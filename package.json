{
  "name": "data-layer",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "tsx node_modules/drizzle-kit/bin.cjs generate",
    "db:migrate": "tsx node_modules/drizzle-kit/bin.cjs migrate",
    "db:push": "tsx node_modules/drizzle-kit/bin.cjs push",
    "db:studio": "tsx node_modules/drizzle-kit/bin.cjs studio",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@fastify/cors": "^11.0.0",
    "@fastify/multipart": "^9.4.0",
    "csv-parse": "^6.1.0",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "exceljs": "^4.1.1",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^4.0.2",
    "googleapis": "^144.0.0",
    "ky": "^1.7.4",
    "mammoth": "^1.11.0",
    "nanoid": "^5.0.9",
    "pdf-parse": "^2.4.5",
    "pg-boss": "^10.1.5",
    "pino": "^9.6.0",
    "playwright-core": "^1.58.2",
    "postgres": "^3.4.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/pdf-parse": "^1.1.5",
    "drizzle-kit": "^0.31.9",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  }
